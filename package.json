{
    "title": "Creative",
    "name": "startbootstrap-creative",
    "version": "5.1.1",
    "description": "Creative is a stylish one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://startbootstrap.com/template-overviews/creative",
    "bugs": {
        "url": "https://github.com/BlackrockDigital/startbootstrap-creative/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (http://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BlackrockDigital/startbootstrap-creative.git"
    },
    "scripts": {
        "preinstall": "mkdir -p dist/",
        "watch-js": "node_modules/.bin/watchify js/creative.js -o dist/bundle.js -v",
        "watch-css": "node_modules/.bin/node-sass -w scss/creative.scss dist/style.css",
        "watch": "yarn run watch-js & yarn run watch-css",
        "build-js": "node_modules/.bin/browserify js/creative.js | node_modules/.bin/uglifyjs -mc > dist/bundle.js",
        "build-css": "node_modules/.bin/node-sass scss/creative.scss dist/style.css --output-style compressed",
        "build": "yarn run build-js | yarn run build-css"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "5.13.1",
        "bootstrap": "4.5.0",
        "jquery": "3.5.1",
        "jquery.easing": "^1.4.1"
    },
    "devDependencies": {
        "browserify": "^16.5.1",
        "node-sass": "^4.14.1",
        "uglify-es": "^3.3.9",
        "watchify": "^3.11.1"
    }
}
